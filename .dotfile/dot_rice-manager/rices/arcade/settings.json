{
  "windowsTheme": "dark",
  "windowsTerminalTheme": "Arcade",
  "windowsTerminalFont": {
    "cellHeight": "1.2",
    "face": "JetBrainsMono NFP",
    "size": 12
  },
  "komorebiConfig": {
    "borderColours": {
      "single": "#00ffd2",
      "stack": "#46dffb",
      "monocle": "#f8ef02",
      "unfocused": "#333333",
      "floating": "#ff00d4"
    },
    "borderWidth": 2,
    "borderStyle": "Square",
    "workspacePadding": 8,
    "containerPadding": 8,
    "workAreaOffset": { "top": 0, "bottom": 0, "left": 0, "right": 0 }
  }
}
