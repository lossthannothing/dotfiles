{
  "windowsTheme": "dark",
  "windowsTerminalTheme": "Jade",
  "windowsTerminalFont": {
    "cellHeight": "1.2",
    "face": "SpaceMono Nerd Font",
    "size": 12
  },
  "komorebiConfig": {
    "borderColours": {
      "single": "#b39a8d",
      "stack": "#b39a8d",
      "monocle": "#b39a8d",
      "unfocused": "#0f1e23",
      "floating": "#b39a8d"
    },
    "borderWidth": 2,
    "borderStyle": "Rounded",
    "workspacePadding": 12,
    "containerPadding": 12,
    "workAreaOffset": {
      "top": 0,
      "bottom": 0,
      "left": 0,
      "right": 0
    }
  }
}
