{
  "windowsTheme": "dark",
  "windowsTerminalTheme": "Horizon Dark",
  "windowsTerminalFont": {
    "cellHeight": "1.2",
    "face": "JuliaMono",
    "size": 13.5
  },
  "komorebiConfig": {
    "borderColours": {
      "single": "#E93C58",
      "stack": "#E93C58",
      "monocle": "#E93C58",
      "unfocused": "#1C1E26",
      "floating": "#E93C58"
    },
    "borderWidth": 2,
    "borderStyle": "Rounded",
    "workspacePadding": 8,
    "containerPadding": 8,
    "workAreaOffset": {
      "top": 0,
      "bottom": 0,
      "left": 0,
      "right": 0
    }
  }
}
