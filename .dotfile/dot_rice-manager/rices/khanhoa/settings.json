{
  "windowsTheme": "light",
  "windowsTerminalTheme": "Catppuccin Latte",
  "windowsTerminalFont": {
    "cellHeight": "1.2",
    "face": "JetBrainsMono NF",
    "size": 12
  },
  "komorebiConfig": {
    "borderColours": {
      "single": "#8087a2",
      "stack": "#a5adcb",
      "monocle": "#627199",
      "unfocused": "#cad3f5",
      "floating": "#f38ba8"
    },
    "borderWidth": 2,
    "borderStyle": "Rounded",
    "workspacePadding": 8,
    "containerPadding": 8,
    "workAreaOffset": { "top": 0, "bottom": 0, "left": 0, "right": 0 }
  }
}
