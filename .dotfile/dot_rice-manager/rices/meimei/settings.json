{
  "windowsTheme": "dark",
  "windowsTerminalTheme": "Gruvbox Dark",
  "windowsTerminalFont": {
    "cellHeight": "1.2",
    "face": "Bahnschrift SemiCondensed",
    "size": 13
  },
  "komorebiConfig": {
    "borderColours": {
      "single": "#FDFF3B",
      "stack": "#ff9f00",
      "monocle": "#4cbb17",
      "unfocused": "#0B0B0B",
      "floating": "#d7453d"
    },
    "borderWidth": 2,
    "borderStyle": "Square",
    "workspacePadding": 8,
    "containerPadding": 8,
    "workAreaOffset": { "top": 0, "bottom": 0, "left": 0, "right": 0 }
  }
}
