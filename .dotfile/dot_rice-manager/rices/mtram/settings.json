{
  "windowsTheme": "dark",
  "windowsTerminalTheme": "Everforest Dark",
  "windowsTerminalFont": {
    "cellHeight": "1.2",
    "face": "JetBrainsMono NF",
    "size": 12
  },
  "komorebiConfig": {
    "borderColours": {
      "single": "#A7C080",
      "stack": "#83C092",
      "monocle": "#7FBBB3",
      "unfocused": "#374145",
      "floating": "#E67E80"
    },
    "borderWidth": 2,
    "borderStyle": "Rounded",
    "workspacePadding": 8,
    "containerPadding": 8,
    "workAreaOffset": { "top": 0, "bottom": 0, "left": 0, "right": 0 }
  }
}
