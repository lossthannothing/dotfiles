{
  "windowsTheme": "dark",
  "windowsTerminalTheme": "Stella",
  "windowsTerminalFont": {
    "cellHeight": "1.1",
    "face": "Monofur Nerd Font Mono",
    "size": 17
  },
  "komorebiConfig": {
    "borderColours": {
      "single": "#c594ff",
      "stack": "#c594ff",
      "monocle": "#c594ff",
      "unfocused": "#2b213c",
      "floating": "#c594ff"
    },
    "borderWidth": 2,
    "borderStyle": "Rounded",
    "workspacePadding": 8,
    "containerPadding": 8,
    "workAreaOffset": {
      "top": 0,
      "bottom": 0,
      "left": 0,
      "right": 0
    }
  }
}
