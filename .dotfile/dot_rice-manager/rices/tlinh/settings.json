{
  "windowsTheme": "dark",
  "windowsTerminalTheme": "Catppuccin Mocha",
  "windowsTerminalFont": {
    "cellHeight": "1.2",
    "face": "JetBrainsMono NF",
    "size": 12
  },
  "komorebiConfig": {
    "borderColours": {
      "single": "#89dceb",
      "stack": "#a6e3a1",
      "monocle": "#cba6f7",
      "unfocused": "#585b70",
      "floating": "#eba0ac"
    },
    "borderWidth": 2,
    "borderStyle": "Rounded",
    "workspacePadding": 8,
    "containerPadding": 8,
    "workAreaOffset": { "top": 0, "bottom": 0, "left": 0, "right": 0 }
  }
}
