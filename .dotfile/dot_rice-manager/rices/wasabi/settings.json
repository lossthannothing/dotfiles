{
  "windowsTheme": "dark",
  "windowsTerminalTheme": "Gruvbox Dark Medium",
  "windowsTerminalFont": {
    "cellHeight": "1.2",
    "face": "Pixel Code, Pixelcraft Nerd Font",
    "size": 13.5
  },
  "komorebiConfig": {
    "borderColours": {
      "single": "#b8bb26",
      "stack": "#b8bb26",
      "monocle": "#b8bb26",
      "unfocused": "#282828",
      "floating": "#b8bb26"
    },
    "borderWidth": 2,
    "borderStyle": "Square",
    "workspacePadding": 8,
    "containerPadding": 8,
    "workAreaOffset": {
      "top": 0,
      "bottom": 0,
      "left": 0,
      "right": 0
    }
  }
}
